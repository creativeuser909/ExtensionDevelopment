{
	"manifest_version": 3,
	"name": "YTOne",
	"description": "Get the coordinate Location.",
	"version": "1.0",
	"permissions": ["storage", "alarms", "activeTab"],
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "/images/ytOne16.png",
		"32": "/images/ytOne32.png",
		"48": "/images/ytOne48.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/images/ytOne16.png",
			"32": "/images/ytOne32.png",
			"48": "/images/ytOne48.png"
		}
	}
}
